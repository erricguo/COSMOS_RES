                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>--本周訂購清單
SELECT COUNT(XC001) XC001,XC004,XA003 XC004C,MAX(XC002) XC002,MAX(XC009) XC009 FROM POSXC 
LEFT JOIN POSXA ON XA001=XC004
WHERE XC002 BETWEEN  CONVERT(varchar(100), DATEADD(week, DATEDIFF(week, '', getdate()), 0), 112) 
AND CONVERT(varchar(100), DATEADD(week, DATEDIFF(week, '', getdate()), 4), 112)
