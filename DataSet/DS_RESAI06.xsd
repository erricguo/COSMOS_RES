                                                              POSXG ON POSXF.XF001 = POSXG.XG001) AS B ON POSXQ.XQ003 = B.XG006 AND 
                            B.XF008 = POSXQ.XQ001
WHERE          (A.XP002 IS NOT NULL) AND (POSXQ.XQ001 = @XQ001)
ORDER BY   POSXQ.XQ003</CommandText>
